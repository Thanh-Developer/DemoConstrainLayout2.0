<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.constraint2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.demo.constraint2.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".constraintset.MovieActivity" />
        <activity android:name=".constraintset.CupcakeActivity" />
        <activity android:name=".circular.CircularActivity" />
        <activity android:name=".miagefilter.ImageFilterViewActivity" />
        <activity android:name=".contrainstate.ConstraintLayoutStatesActivity" />
        <activity android:name=".motionlayout.swipe.RecyclerViewSwipeMotionActivity" />
        <activity
            android:name=".motionlayout.cordinator.MotionActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".motionlayout.keyframe.MotionKeyFrameActivity" />
        <activity android:name=".motionlayout.basic.MotionBasicActivity" />
        <activity android:name=".recyclerperconstrain.ConstrainActivity" />
        <activity android:name=".recyclerliner.LinearActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>